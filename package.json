{
  "name": "js-dmw",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "flow-remove-types src/ -d out/",
    "buildWithMaps": "flow-remove-types src/ -d out/ --pretty --sourcemaps",
    "start": "yarn run build && node out/get_parameters.js",
    "start-db": "yarn run build && node out/dbaccess.js",
    "nodemon": "nodemon --watch src --delay 1 --exec yarn start",
    "addTypes": "flow-typed install",
    "flow": "flow",
    "eslint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Doni Setiawan <donisetiawan.050@gmail.com>",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-flow"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "flowtype"
    ],
    "rules": {
      "no-console": "off"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.2.1",
    "eslint-config-recommended": "^4.0.0",
    "eslint-plugin-flowtype": "^4.2.0",
    "flow-bin": "^0.105.2",
    "flow-remove-types": "^2.106.0",
    "flow-typed": "^2.6.1",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "fetch": "^1.1.0",
    "helmet": "^3.20.0",
    "moment": "^2.24.0"
  }
}
